Group {
  LayoutMode: Left;
  Anchor: (Height: 30, Left: 0, Right: 0);
  Background: #17374f(0.78);
  Padding: (Left: 8, Right: 8);

  Label #SectionTitle {
    Text: "Section";
    Anchor: (Left: 0, Right: 0, Height: 30);
    Style: (FontSize: 12, TextColor: #f0dfb3, RenderBold: true, RenderUppercase: true, VerticalAlignment: Center);
  }
}
